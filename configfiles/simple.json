{
    "requestLogger": {
        "requestLogFile": "request.log",
        "maxSizeMegabytes": 1,
        "maxBackups": 10
    },
    "servers": [
        {
            "serverID": "http",
            "networkAndListenAddressList": [
                {
                    "network": "tcp",
                    "listenAddress": ":8080"
                }
            ],
            "timeouts": {
                "readTimeoutMilliseconds": 30000,
                "writeTimeoutMilliseconds": 30000
            },
            "locations": [
                {
                    "httpPathPrefix": "/.well-known/acme-challenge/",
                    "directoryLocation": {
                        "stripPrefix": "/.well-known/acme-challenge/",
                        "directoryPath": "/Users/aaron/acme",
                        "cacheControlValue": "no-cache"
                    }
                },
                {
                    "httpPathPrefix": "",
                    "directoryLocation": {
                        "directoryPath": ".",
                        "cacheControlValue": "public, max-age=150"
                    }
                }
            ]
        }
    ]
}