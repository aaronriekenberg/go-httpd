{
    "servers": [
        {
            "serverID": "http",
            "listenAddressList": [
                ":8080"
            ],
            "httpServerTimeouts": {
                "readTimeoutMilliseconds": 30000,
                "writeTimeoutMilliseconds": 30000
            },
            "logRequests": true,
            "locations": [
                {
                    "httpPathPrefix": "/.git",
                    "blockedLocation": {
                        "responseStatus": 403
                    }
                },
                {
                    "httpPathPrefix": "/.well-known/acme-challenge/",
                    "directoryLocation": {
                        "stripPrefix": "/.well-known/acme-challenge/",
                        "directoryPath": "/Users/aaron/acme"
                    }
                },
                {
                    "httpPathPrefix": "",
                    "redirectLocation": {
                        "redirectURL": "https://localhost:8443$REQUEST_PATH",
                        "responseStatus": 302
                    }
                }
            ]
        },
        {
            "serverID": "https",
            "listenAddressList": [
                ":8443"
            ],
            "tlsInfo": {
                "certFile": "cert.pem",
                "keyFile": "key.pem"
            },
            "httpServerTimeouts": {
                "readTimeoutMilliseconds": 30000,
                "writeTimeoutMilliseconds": 30000
            },
            "logRequests": true,
            "locations": [
                {
                    "httpPathPrefix": "/.git",
                    "blockedLocation": {
                        "responseStatus": 403
                    }
                },
                {
                    "httpPathPrefix": "/.well-known/acme-challenge/",
                    "directoryLocation": {
                        "stripPrefix": "/.well-known/acme-challenge/",
                        "directoryPath": "/Users/aaron/acme"
                    }
                },
                {
                    "httpPathPrefix": "/cgi-bin/",
                    "fastCGILocation": {
                        "unixSocketPath": "/tmp/go-fastcgi.sock"
                    }
                },
                {
                    "httpPathPrefix": "/",
                    "directoryLocation": {
                        "directoryPath": "/Users/aaron"
                    }
                }
            ]
        }
    ]
}